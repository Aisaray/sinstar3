        <imgbtn pos="-0,|0" offset="-1,-0.5" name="btn_close" skin="_skin.sys.btn.close" tip="close"/>
      </caption2>
      <window size="-2,0" weight="1" layout="hbox" colorBkgnd="#eef2f7">
        <listbox size="100dp,-2" name="lst_catalog" text-x="16"/>
        <window name="cell_parent" size="0,-2" weight="1" layout="gridlayout" columnCount="10" trackMouseEvent="1" xInterval="1" yInterval="1" xGravity="fill" yGravity="fill" margin="1,1,1,1" colorBorder="#95b7e4" colorBkgnd="#95b7e4">
